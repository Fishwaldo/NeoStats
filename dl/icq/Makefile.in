INCLUDES = -I../..

SOURCES= icq.c icqlib.c
OBJECTS= icq.o icqlib.o
TARGET= icq.so

CC=@CC@
CFLAGS=@CFLAGS@ -fPIC
LINKER=ld

all:
	$(CC) $(CFLAGS) $(INCLUDES) -c $(SOURCES)
	$(LINKER) -shared -o $(TARGET) $(LIBS) $(OBJECTS)
	/bin/cp $(TARGET) ../

clean:
	/bin/rm -rf ../$(TARGET) Makefile
	/bin/rm -rf *.o *.lo *.so